<%= form_for @restaurant do |f| %>
  <label>Name:</label><br>
  <%= f.text_field :name %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>

  <%= f.label :city %><br>
  <%= f.text_field :city %><br>

  <%= f.label :star_rating %><br>
  <%= f.number_field :star_rating %><br>

  <%= f.label :cuisine %><br>
  <%= f.text_field :cuisine %><br>

  <%= f.submit %><br>

<% end %>